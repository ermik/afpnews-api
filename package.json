{
  "name": "afpnews-api",
  "version": "1.7.0",
  "description": "Node helper functions to authenticate and fetch AFP News API",
  "main": "dist/afpnews-api.js",
  "umd:main": "dist/afpnews-api.umd.js",
  "module": "dist/afpnews-api.es.js",
  "source": "src/afpnews-api.ts",
  "typings": "dist/afpnews-api.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build:d": "tsc --outFile dist/afpnews-api.js --module commonjs --target es2015 --declaration --emitDeclarationOnly",
    "build:lib": "tsc --outDir dist/lib --module esnext --target esnext",
    "microbundle:web": "microbundle -i dist/lib/afpnews-api.js -o dist -f es,umd --target web --strict --name AfpNews --sourcemap --globals isomorphic-base64=window,isomorphic-fetch=fetch,isomorphic-form-data=FormData",
    "microbundle:node": "microbundle -i dist/lib/afpnews-api.js -o dist -f cjs --target node --strict --sourcemap",
    "build": "npm run clean && npm run build:d && npm run build:lib && npm run microbundle:web && npm run microbundle:node",
    "clean": "rm -rf dist .rts2_cache_*",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "test": "jest --coverage"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julesbonnard/afpnews-api.git"
  },
  "keywords": [
    "AFP",
    "News",
    "API",
    "Node"
  ],
  "author": "Jules Bonnard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julesbonnard/afpnews-api/issues"
  },
  "homepage": "https://github.com/julesbonnard/afpnews-api#readme",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "dotenv": "^7.0.0",
    "jest": "^24.5.0",
    "lucene": "^2.0.3",
    "microbundle": "^0.11.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^11.11.3",
    "isomorphic-base64": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  }
}
