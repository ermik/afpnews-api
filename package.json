{
  "name": "afpnews-api",
  "version": "1.2.1",
  "description": "Node helper functions to authenticate and fetch AFP News API",
  "main": "dist/afpnews-api.node.js",
  "module": "src/index.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist stats.json",
    "dev": "webpack --mode development --watch --display-error-details",
    "build": "webpack --mode production",
    "analyse": "webpack --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json",
    "test": "mocha --colors --exit --timeout 5000 ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julesbonnard/afpnews-api.git"
  },
  "keywords": [
    "AFP",
    "News",
    "API",
    "Node"
  ],
  "author": "Jules Bonnard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julesbonnard/afpnews-api/issues"
  },
  "homepage": "https://github.com/julesbonnard/afpnews-api#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-recommended": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "btoa": "^1.2.1",
    "form-data": "^2.3.2",
    "lucene-query-parser": "^1.2.0"
  }
}
